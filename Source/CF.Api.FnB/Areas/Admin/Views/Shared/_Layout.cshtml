<!DOCTYPE html>

<html>
<head>
    <title>@ViewBag.Title</title>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <!-- Tell the browser to be responsive to screen width -->
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <meta name="description" content="">
    <meta name="author" content="">
    <!-- Favicon icon -->
    <link rel="icon" type="image/png" sizes="16x16" href="@Url.Content("~/Areas/Admin/images/favicon.png")">
    <!-- Bootstrap Core CSS -->
    <link href="@Url.Content("~/Areas/Admin/css/lib/bootstrap/bootstrap.min.css")" rel="stylesheet">

    <!-- Custom CSS -->
    <link href="@Url.Content("~/Areas/Admin/css/helper.css")" rel="stylesheet">
    <link href="@Url.Content("~/Areas/Admin/css/style.css")" rel="stylesheet">
    <link href="@Url.Content("~/Lib/plugin/icheck/skins/all.css")" rel="stylesheet" />
    @*<link href="@Url.Content("~/Lib/css/components.min.css")" rel="stylesheet" />*@

    <!-- All Jquery -->
    <script src="@Url.Content("~/Areas/Admin/js/lib/jquery/jquery.min.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/js/jquery.slimscroll.js")"></script>
    <script src="@Url.Content("~/Areas/Admin/js/jquery.slimscroll.js")"></script> <!-- slimscrollbar scrollbar JavaScript -->
    <!--Custom JavaScript -->
    <script src="@Url.Content("~/Areas/Admin/js/custom.min.js")"></script>

    <script src="@Url.Content("~/Scripts/common.js")"></script>

    <!-- Bootstrap tether Core JavaScript -->
    <script src="@Url.Content("~/Areas/Admin/js/lib/bootstrap/js/popper.min.js")"></script>

    <!-- slimscrollbar scrollbar JavaScript -->
    <!--Menu sidebar -->
    <script src="@Url.Content("~/Areas/Admin/js/sidebarmenu.js")"></script>
    <!--stickey kit -->
    <script src="@Url.Content("~/Areas/Admin/js/lib/sticky-kit-master/dist/sticky-kit.min.js")"></script>


    <script src="@Url.Content("~/Lib/plugin/icheck/icheck.min.js")"></script>

    @*<script src="@Url.Content( "~/Scripts/jquery.validate.min.js")"></script>
    <script src="@Url.Content( "~/Scripts/jquery.validate.unobtrusive.min.js")"></script>*@

</head>
<body class="page-header-fixed page-sidebar-closed-hide-logo page-content-white">
    <div id="main-wrapper">
        <!-- BEGIN HEADER & CONTENT DIVIDER -->
        @Html.Partial("_topMenu")
        <div class="clearfix"> </div>
        <!-- END HEADER & CONTENT DIVIDER -->
        <div class="left-sidebar">
            @Html.Partial("_leftMenu")
        </div>

        <!-- BEGIN CONTAINER -->
        <div class="page-wrapper">
            <!-- BEGIN CONTENT -->
            <!-- BEGIN PAGE HEADER-->
            @Html.Partial("_BreadCrumb")
            <!-- END PAGE HEADER-->
            @RenderBody()
            <!-- END CONTENT -->
            <!-- BEGIN FOOTER -->
            @Html.Partial("_Footer")
            <!-- END FOOTER -->
        </div>
        <!-- END CONTAINER -->

    </div>


    <!-- END CONTAINER -->

    <script src="@Url.Content("~/Areas/Admin/js/lib/bootstrap/js/bootstrap.min.js")"></script>

    <script type="text/javascript">
        $(document).ready(function () {

        })
    </script>

</body>
</html>

